<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.flyhz.shop.persistence.dao.ConsigneeDao">
	<insert id="insertConsignee">
		insert into consignee(name,contury,city,province,address,zipcode,mobilephone,user_id) 
		values(#{name},#{contury},#{city},#{province},#{address},#{zipcode},#{mobilephone},#{userId})
	</insert>
</mapper>